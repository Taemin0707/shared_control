<launch>

  <arg name="robot_pose_x" value="4.5"/>
  <arg name="robot_pose_y" value="1.5"/>
  <arg name="robot_pose_Y" value="1.57"/>

  # 환경 시뮬레이션
  <include file="$(find shared_control)/launch/gazebo.launch">
    <arg name="robot_pose_x" value="$(arg robot_pose_x)"/>
    <arg name="robot_pose_y" value="$(arg robot_pose_y)"/>
    <arg name="robot_pose_Y" value="$(arg robot_pose_Y)"/>
  </include>

  # 이동로봇 제어
  <node pkg="shared_control" type="direct_controller.py" name="direct_controller" output="screen"/>
  <node pkg="shared_control" type="local_path_planner.py" name="local_path_planner" output="screen"/>

  # 인터페이스
  <!-- <node pkg="rviz" type="rviz" name="rviz"
        args="-d $(find shared_control)/rviz/gazebo_test.rviz"/> -->

</launch>
